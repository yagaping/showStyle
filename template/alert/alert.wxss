/* template/alert/alert.wxss */
.mask{
 width: 100%;
 height: 100%;
 position: fixed;
 top:0;
 left:0;
 right:0;
 bottom:0;
 background: rgba(0,0,0,.4);
 z-index:99;
}
.alert{
  position: fixed;
  top:30%;
  left:50%;
  z-index:100;
  background: #fff;
  border-radius: 11rpx;
}
.a_title{
  text-align: center;
  font-size:26rpx;
}
.a_tel{
  padding:0 20rpx;
  overflow: hidden;
}